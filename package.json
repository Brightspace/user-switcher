{
  "name": "user-switcher",
  "description": "Brightspace User Switcher",
  "private": true,
  "scripts": {
    "postinstall": "polymer install --variants",
    "test": "npm run test:lint && npm run test:polymer",
    "test:lint": "npm run test:lint:html && npm run test:lint:wc",
    "test:lint:html": "eslint --ext ./*.js,./*.html .",
    "test:lint:wc": "polymer lint -i d2l-user-switcher.html d2l-user-switcher-item.html",
    "test:polymer": "polymer test",
    "test:polymer:local": "cross-env LAUNCHPAD_BROWSERS=chrome polymer test -p --skip-plugin sauce"
  },
  "homepage": "https://github.com/Brightspace/user-switcher",
  "repository": {
    "type": "git",
    "url": "https://github.com/Brightspace/user-switcher.git"
  },
  "bugs": {
    "url": "https://github.com/Brightspace/user-switcher/issues"
  },
  "keywords": [
    "d2l",
    "user",
    "switcher"
  ],
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "devDependencies": {
    "cross-env": "^5.1.4",
    "eslint": "^4.13.1",
    "eslint-config-brightspace": "^0.4.0",
    "eslint-plugin-html": "^4.0.1",
    "polymer-cli": "^1.7.7"
  }
}
